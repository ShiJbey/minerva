{% if character.biological_father is not none %}
<a href="/gameobject?uid={{character.biological_father.uid}}">{{character.biological_father.name}} ({{character.biological_father.uid}})</a>
{% else %}
N/A
{% endif %}
<br>
<font size="4"><b>Siblings: </b></font>
{% if character.siblings %}
{% for character in character.siblings%}
- <a href="/gameobject?uid={{character.uid}}">{{character.name}} ({{character.uid}})</a><br>
{% endfor %}
{% else %}
N/A<br>
{% endif %}
